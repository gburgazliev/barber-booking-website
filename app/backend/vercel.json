{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": { "cache-control": "s-maxage=0" },
        "dest": "/index.html"
      }
    ]
  }